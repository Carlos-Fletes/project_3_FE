{
  "expo": {
    "name": "Project 3 Frontend",
    "slug": "project-3-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourname.project3fe",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "scheme": "project3fe",
    "deepLinks": [
      "auth"
    ],
    "plugins": [
      "expo-dev-client",
      "expo-secure-store"
    ],
    "extra": {
      "backendUrl": "https://project-3-backend.herokuapp.com",
      "eas": {
        "projectId": "06116d5c-c203-45d3-830d-0f18adf9fb1b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    }
  }
}
